{
  "expo": {
    "name": "CityWise",
    "slug": "citywise-transit-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "citywise",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow CityWise to access your camera to scan QR codes."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}